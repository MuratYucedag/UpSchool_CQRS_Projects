@model GetProductAccounterByIDQueryResult

@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

<div class="container">
    <h2>Muhasebeci Ürün Detay Paneli</h2>
    <label>Ürün ID</label>
    <input type="text" asp-for="ProductID" class="form-control" />
    <br />
    <label>Ürün Adı</label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />
    <label>Ürün Markası</label>
    <input type="text" asp-for="Brand" class="form-control" />
    <br />
    <label>Ürün Fiyatı</label>
    <input type="text" asp-for="SalePrice" class="form-control" />
    <br />
    <label>Açıklama</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br />
</div>